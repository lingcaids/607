   "source": [
    "size = 400\n",
    "# t = np.arange(-1, 1.01, 0.01)\n",
    "m = np.sqrt(2/np.pi)\n",
    "X = np.array([np.random.uniform(0, 2, size=size), np.random.uniform(-1, 1, size=size)]).T\n",
    "Z = X[:, 0]**2 - 2*m*X[:, 0] + X[:, 1]**2\n",
    "I = np.abs(Z) >= 0.01\n",
    "X = X[I]\n",
    "Z = Z[I]\n",
    "y = np.sign(Z)\n",
    "\n",
    "XX = np.hstack([X[:, 0].reshape(-1, 1), X**2])\n",
    "\n",
    "w, ws = perceptron(XX, y)\n",
    "\n",
    "u = np.arange(0, 2, 0.01)\n",
    "v = np.arange(-1, 1, 0.01)\n",
    "U, V = np.meshgrid(u, v)\n",
    "W = w[0]*U + w[1]*U**2 + w[2]*V**2\n",
    "\n",
    "plt.figure(figsize=(6, 6))\n",
    "plt.plot(X[y == -1, 0], X[y == -1, 1], 'bo')\n",
    "plt.plot(X[y == 1, 0], X[y == 1, 1], 'ro')\n",
    "plt.contour(u, v, W, levels=[0], linewidths=[2], colors=['k'])\n",
    "plt.show()"
   ]
